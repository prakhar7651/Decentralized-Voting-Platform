            <button  type="button" class="btn btn-outline-primary" style="display:inline-block;" id="myInput" onclick="myFunction()">Copy text</button>
            <div style="display: inline-block; width: 5px;"></div>
            <form style="display:inline-block;" method="get" action = <%= action_path %>>
              <button type="submit" class="btn btn-outline-primary" style="display:inline-block;">Proceed</button>
            </form>
            </center>

