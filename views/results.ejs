    <div style="margin: auto; width: 75rem;">
        <table class="table">
            <thead class="thead-dark">
              <tr>
                <th scope="col">Department Name</th>
                <th scope="col">Votes Recieved</th>
              </tr>
            </thead>
            <tbody>
              <% for(var i = 0; i < candidates.length; i++){ %>
                <tr>
                  <td><%= candidates[i]['name'] %></td>
                  <td><%= candidates[i]['votes'] %></td>
                </tr>
              <% } %>
            </tbody>
          </table>
